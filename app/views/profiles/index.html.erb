<p id="notice"><%= notice %></p>

<h1 class= "text-center">All Wyncoders</h1>
  <div class="container">
  <div class="row">
    <% @profiles.each do |profile| %>
    <div class="col-xs-6 col-md-3">
      <div class="thumbnail">
        <img src="http://placehold.it/200x200" alt="...">
        <div class="caption">
          <h2>Profile Name</h2>
          <%= button_to 'Show Profile', profile, :class => "btn btn-primary", :method => :get %>
        </div> 
      </div>
    </div>
    <% end %>
  </div>
</div>